[
    {
        "name": "hello_world",
        "image": "hello-world",
        "cpu": 1,
        "memory": 128,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "ap-southeast-2",
                "awslogs-group": "hello_world",
                "awslogs-stream-prefix": "complete-ecs"
            }
        },
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp"
            }
        ]
    }
]
